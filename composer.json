                "BionConnection\\EmnifyAPI\\EmnifyAPIServiceProvider"
            ],
            "aliases": {
                "EmnifyAPI": "BionConnection\\EmnifyAPI\\Facades\\EmnifyAPI"
            }
        }
    }
